{
  "manifest_version": 2,
  "name" : "Slackmarks",
  "description" : "Sends Chrome Page to Slack",
  "version" : "0.0.1",
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "background" : {
    "persistent" : true,
    "scripts" : ["config.js","bookmark.js"]
  },
  "commands": {
		"bookmark_page": {
			"suggested_key": {
				"default": "Ctrl+B"
			},
			"description": "Bookmark Page"
		}
	}
}
